{
  "private": true,
  "version": "2.98.0",
  "name": "@8x8/oxygen",
  "scripts": {
    "clear": "yarn clear:dist && yarn clear:dependencies",
    "clean": "lerna clean -y",
    "clear:dist": "rimraf \"./packages/*/dist\" \"./packages/icon/novo\" \"./packages/icon/icons\" \"./packages/icon/utils\" || true",
    "clear:dependencies": "rimraf ./packages/**/node_modules/ && rimraf ./node_modules/",
    "build:js": "lerna exec --ignore \"@8x8/oxygen-{switchBase,checkbox,toggleButton,storybook-utils,sidebar-menu,label,user-status,tag,select,table-of-contents,icon,config,constants,utils,theme,tooltip,static-tooltip,accordion,avatar,button,text-link,link,popover,icon-button,alert-banner,tabs,toast,loaders,keyword-search,dropdown-button,filter-list,pagination,skeletons,columnManagement,calendar,input,slider,badge,graphics,data-table,treeshake}\" --parallel -- babel --root-mode upward src -d dist --ignore \"**/*.test.js,**/*.stories.js,**/*.demo.js\"",
    "build:ts": "tsc -b packages/theme packages/constants packages/utils packages/config packages/staticTooltip packages/tooltip packages/icon packages/storybookUtils packages/button packages/accordion packages/userStatus packages/avatar packages/label packages/textLink packages/link packages/popover packages/iconButton packages/alertBanner packages/tabs packages/loaders packages/toast packages/tableOfContents packages/input packages/keywordSearch packages/dropdownButton packages/switchBase packages/checkbox packages/toggleButton packages/tag packages/select packages/filterList packages/pagination packages/skeletons packages/columnManagement packages/calendar packages/slider packages/badge packages/dataTable packages/graphics packages/sidebarMenu",
    "build:copy-types": "lerna exec --parallel --ignore \"@8x8/oxygen-{switchBase,checkbox,toggleButton,storybook-utils,sidebar-menu,label,user-status,tag,select,table-of-contents,icon,config,constants,utils,theme,tooltip,static-tooltip,accordion,avatar,button,text-link,link,popover,icon-button,alert-banner,tabs,toast,loaders,keyword-search,dropdown-button,filter-list,pagination,skeletons,columnManagement,calendar,input,slider,badge,graphics,data-table}\" -- cpx src/index.d.ts dist",
    "build": "yarn clear:dist && yarn build:js && yarn build:copy-types && yarn build:ts && yarn build:copy-icon && yarn build:copy-old-graphics",
    "build:dev": "yarn clear:dist && yarn build:js --watch & yarn build:copy-types --watch & yarn build:ts --watch",
    "build:storybook": "rimraf ./storybook && storybook build -c .storybook -o storybook",
    "exportThemeJson": "yarn lerna exec yarn generateJSON --scope @8x8/oxygen-theme",
    "build:copy-icon": "cpx 'packages/icon/dist/src/novo/*.{js,ts,map}' packages/icon/novo && cpx 'packages/icon/src/novo/*.svg' packages/icon/novo && cpx 'packages/icon/dist/src/icons/*.{js,ts,map}' packages/icon/icons && cpx 'packages/icon/src/icons/*.svg' packages/icon/icons && cpx 'packages/icon/dist/src/utils/*.{js,ts,map}' packages/icon/utils",
    "build:copy-old-graphics": "cpx 'packages/graphics/dist/src/*.{js,ts,map}' packages/graphics/dist",
    "storybook": "storybook dev -p 9001 -c .storybook",
    "update:version": "lerna version --no-push --force-publish --yes --exact --message \"[NO-TICKET] Updated version to %v\"",
    "changelog:update-version": "node ./scripts/updateChangelogVersion.js",
    "changelog": "node ./scripts/generateChangelog.js",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:stylelint --fix",
    "lint:eslint": "yarn eslint .",
    "lint:stylelint": "stylelint '**/*.{js,tsx}'",
    "test": "jest --config jest.json",
    "test:ci": "node --no-compilation-cache --expose-gc ./node_modules/.bin/jest --config jest.json --coverage",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:treeshake": "yarn build && yarn lerna exec yarn build --scope @8x8/oxygen-treeshake",
    "lerna:version": "lerna -v",
    "lerna:publish": "lerna publish --no-git-tag-version --no-push --force-publish --yes --exact",
    "create:release": "node scripts/release/createRelease.mjs",
    "link:all": "lerna exec -- yarn link",
    "unlink:all": "lerna exec -- yarn unlink"
  },
  "resolutions": {
    "@babel/plugin-transform-block-scoping": "7.20.5",
    "chokidar": "^3.4.0"
  },
  "devDependencies": {
    "@8x8/oxygen-constants": "*",
    "@babel/cli": "7.24.1",
    "@babel/core": "7.24.4",
    "@babel/eslint-parser": "7.24.1",
    "@babel/eslint-plugin": "7.23.5",
    "@babel/preset-env": "7.24.4",
    "@babel/preset-react": "7.24.1",
    "@babel/preset-typescript": "7.24.1",
    "@storybook/addon-a11y": "9.1.1",
    "@storybook/addon-console": "3.0.0",
    "@storybook/addon-docs": "9.1.1",
    "@storybook/addon-jest": "9.1.1",
    "@storybook/addon-links": "9.1.1",
    "@storybook/builder-vite": "9.1.1",
    "@storybook/react": "9.1.1",
    "@storybook/react-vite": "9.1.1",
    "@swc/core": "1.4.12",
    "@swc/jest": "0.2.36",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/enzyme": "^3.10.18",
    "@types/jest": "29.5.12",
    "@types/lodash.assign": "4.2.7",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/lodash.debounce": "4.0.7",
    "@types/lodash.isempty": "4.4.7",
    "@types/lodash.isfunction": "3.0.7",
    "@types/lodash.isobject": "3.0.7",
    "@types/lodash.isundefined": "3.0.9",
    "@types/react-test-renderer": "17.0.2",
    "@types/resize-observer-browser": "0.1.7",
    "@types/styled-components": "5.1.34",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.7.0",
    "babel-jest": "29.7.0",
    "chalk": "5.3.0",
    "changelog-parser": "2.8.1",
    "chromatic": "6.24.1",
    "co": "4.6.0",
    "compare-versions": "5.0.3",
    "cpx2": "8.0.0",
    "css-loader": "6.11.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eol": "0.9.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "5.0.0",
    "fast-uri": "^3.0.6",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-styled-components": "7.2.0",
    "lerna": "7.4.2",
    "load-json-file": "7.0.1",
    "mockdate": "3.0.5",
    "polished": "4.2.2",
    "postcss": "8.4.38",
    "postcss-styled-syntax": "0.4.0",
    "prettier": "2.8.8",
    "prettier-eslint": "15.0.1",
    "raw-loader": "^4.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "8.0.7",
    "react-test-renderer": "17.0.2",
    "remark-gfm": "3.0.1",
    "resize-observer-polyfill": "1.5.1",
    "rimraf": "6.0.1",
    "storybook": "9.1.1",
    "style-loader": "3.3.4",
    "styled-components": "5.3.11",
    "styled-tools": "1.7.2",
    "stylelint": "16.3.1",
    "stylelint-config-standard": "36.0.0",
    "stylelint-processor-styled-components": "1.10.0",
    "tsx": "4.19.3",
    "typescript": "4.9.5",
    "url-loader": "4.1.1",
    "vite": "7.0.6",
    "webpack": "5.91.0",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "6.0.1",
    "write-json-file": "5.0.0",
    "yargs": "17.7.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=22.17.0",
    "yarn": ">=4.9.2"
  },
  "packageManager": "yarn@4.9.2"
}
